{
  "name": "ui-builder-code-generator",
  "version": "1.0.0",
  "main": "run.ts",
  "license": "MIT",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "copy-templates": "yarn copy-templates:server && yarn copy-templates:client",
    "copy-templates:server": "cp -r src/ServerGenerator/templates build/ServerGenerator/templates",
    "copy-templates:client": "cp -r src/ClientGenerator/templates build/ClientGenerator/templates",
    "build": "rm -rf build/* && tsc && yarn copy-templates"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/pg": "^7.14.4",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettycli": "^1.4.3",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "graph-data-structure": "^1.12.1",
    "mustache": "^4.0.1",
    "pg": "^8.3.0"
  }
}
