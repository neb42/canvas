const initialState = {
  <% #widgets %>
  <% name %>: {
  <% #properties %>
    <% name %>: <% defaultValue %>,
  <% /properties %>
  },
  <% /widgets %>
};

const widget = (state = initialState, action) => {
  switch (action.type) {
    case 'UPDATE_WIDGET': {
      return {
        ...state,
        [action.name]: {
          ...action.payload,
        },
      };
    }
    default:
      return state;
  }
};

export default widget;