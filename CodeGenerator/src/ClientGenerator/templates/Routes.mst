{{=<% %>=}}

import React from 'react';
import { Route, Switch, BrowserRouter as Router } from 'react-router';

<% #pages %>
import <% name %> from './pages/<% name %>';
<% /pages %>

const Routes = ({ history, store }) => (
  <Router>
    <Switch>
      <% #pages %>
        <Route path="/<% name %>" component={<% name %>} />
      <% /pages %>
    </Switch>
  </Router>
);

export default Routes;