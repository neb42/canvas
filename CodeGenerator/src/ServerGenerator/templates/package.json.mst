{
  "name": "{{{ appName }}}-server",
  "scripts": {
    "build": "babel --quiet -d {{{ buildDirectory }}} {{{ sourceDirectory }}}"
  },
  "dependencies": {
    {{ #dependencies }}
    "{{{ name }}}": "{{{ version }}}"{{ ^last }}, {{ /last }}     
    {{ /dependencies }}
  },
  "devDependencies": {
    {{ #devDependencies }}
    "{{{ name }}}": "{{{ version }}}"{{ ^last }}, {{ /last }}      
    {{ /devDependencies }}
  }
}