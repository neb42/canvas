{{=<% %>=}}

import mysql from 'mysql';

<% #datasets %>
const <% ds.name %> = mysql.createConnection({
  host: '<% ds.host %>',
  port: <% ds.port %>,
  user: '<% ds.user %>',
  password: '<% ds.password %>',
  database: '<% ds.database %>',
});
<% ds.name %>.connect();
export <% ds.name %>;
<% /datasets %>