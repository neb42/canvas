{{=<% %>=}}
import { <% dataset %> } from '../datasets';
<% #serverFunctions %>
import <% . %> from '../serverFunctions/<% . %>';
<% /serverFunctions %>
<% #queries %>
import <% . %> from './<% . %>';
<% /queries %>

const <% name %> = ({
  <% #clientDependencies %>
  <% . %>,
  <% /clientDependencies %> 
}) => {
  return <% dataset %>.query(`<% queryString %>`, (error, results, fields) => {
    if (error) throw error;
    return results;
  });
};

export default <% name %>;