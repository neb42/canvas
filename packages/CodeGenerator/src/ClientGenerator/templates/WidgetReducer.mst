const initialState = {
  {{ #widgets }}
  '{{ id }}': {
  {{ #properties }}
    '{{ id }}': {{{ defaultValue }}},
  {{ /properties }}
  },
  {{ /widgets }}
};

const widget = (state = initialState, action) => {
  switch (action.type) {
    case 'UPDATE_WIDGET': {
      return {
        ...state,
        [action.id]: {
          ...action.payload,
        },
      };
    }
    default:
      return state;
  }
};

export default widget;