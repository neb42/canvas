{{=<% %>=}}
import React from 'react';
import { Route, Switch } from 'react-router-dom';

<% #pages %>
import page_<% name %> from './pages/<% name %>';
<% /pages %>

const Routes = () => (
  <Switch>
    <% #pages %>
    <Route path="/<% route %>" component={page_<% name %>} />
    <% /pages %>
  </Switch>
);

export default Routes;