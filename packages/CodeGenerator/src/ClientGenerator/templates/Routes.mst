{{=<% %>=}}
import React from 'react';
import { Route, Switch, BrowserRouter as Router } from 'react-router-dom';

<% #pages %>
import page_<% name %> from './pages/<% name %>';
<% /pages %>

const Routes = () => (
  <Router>
    <Switch>
      <% #pages %>
      <Route path="/<% name %>" component={page_<% name %>} />
      <% /pages %>
    </Switch>
  </Router>
);

export default Routes;