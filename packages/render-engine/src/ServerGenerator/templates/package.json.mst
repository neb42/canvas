{
  "name": "app-server",
  "scripts": {
    "clean": "rm -rf build/*",
    "build": "yarn clean && tsc",
    "dev": "yarn build && node build/router.js"
  },
  "dependencies": {
    {{ #dependencies }}
    "{{{ name }}}": "{{{ version }}}"{{ ^last }}, {{ /last }}     
    {{ /dependencies }}
  },
  "devDependencies": {
    {{ #devDependencies }}
    "{{{ name }}}": "{{{ version }}}"{{ ^last }}, {{ /last }}      
    {{ /devDependencies }}
  }
}